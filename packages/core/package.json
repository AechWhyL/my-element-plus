{
  "name": "hyl-fake-element-plus",
  "version": "1.2.1",
  "description": "vue3 component library",
  "main": "./dist/umd/index.umd.js",
  "module": "./dist/es/index.es.js",
  "type": "module",
  "types": "./dist/types/core/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/es/index.es.js",
      "require": "./dist/umd/index.umd/js",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "scripts": {
    "build": "run-p build-umd build-es",
    "build-umd": "rimraf dist/umd && vite build --config ./vite.umd.config.ts",
    "build-es": "rimraf dist/es && vite build --config ./vite.es.config.ts",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.12.4",
  "dependencies": {
    "@hyl-fake-element-plus/components": "workspace:*"
  },
  "devDependencies": {}
}
